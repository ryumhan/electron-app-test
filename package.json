{
  "name": "inspection-svm",
  "version": "1.6.0",
  "private": true,
  "dependencies": {
    "@electron/rebuild": "^3.6.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.5.5",
    "axios": "^1.5.0",
    "child_process": "^1.0.2",
    "dayjs": "^1.11.10",
    "electron-is-dev": "^2.0.0",
    "emotion-reset": "^3.0.1",
    "multicast-service-discovery": "^4.0.4",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-app-rewired": "5.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.16.0",
    "recoil": "^0.7.7",
    "sqlite3": "^5.1.7",
    "typescript": "^4.4.2",
    "util": "^0.12.5",
    "web-vitals": "^2.1.0",
    "ws": "^8.14.1"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "productName": "inspection-app",
    "asar": false,
    "appId": "org.cpro95.inspection-app",
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "public/avikus_logo.png"
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./dist/",
      "app": "."
    }
  },
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "python-build": "pyinstaller --onefile ./public/oru_ip_find.py",
    "start": "tsc ./public/electron.ts && concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://127.0.0.1:3000 && electron .\"",
    "build": "npm run react-build && electron-builder",
    "build-arc86": "npm run react-build && electron-builder --x86",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "release": "npm run react-build && electron-builder --publish=always",
    "package": "npm run react-build && electron-packager ./ MyApp --platform=win32 --arch=x64",
    "lint": "eslint './src**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron/remote": "^2.0.11",
    "@emotion/babel-plugin": "^11.11.0",
    "@types/axios": "^0.14.0",
    "@types/downloadjs": "^1.4.4",
    "@types/ip": "^1.1.0",
    "@types/papaparse": "^5.3.9",
    "@types/ping": "^0.4.1",
    "@types/pngjs": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@yarnpkg/sdks": "^3.0.0-rc.50",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "electron-packager": "^17.1.2",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "latest",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "latest",
    "prettier": "^3.0.3",
    "react-app-rewired": "^2.2.1",
    "wait-on": "^7.0.1"
  },
  "packageManager": "yarn@3.6.3"
}
